{
	"name": "cdn.wolfstar.rocks",
	"author": "WolfStar Project",
	"description": "CDN is a content delivery network that is optimized for delivering content to users with high availability and high performance.",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "bun run ./dist/server/entry.mjs",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"lint": "eslint --fix ",
		"format": "prettier --write --log-level=warn \"{src}/**/*\"",
		"prepare": "husky"
	},
	"dependencies": {
		"@astrojs/check": "^0.8.3",
		"@astrojs/sitemap": "^3.1.6",
		"@astrojs/ts-plugin": "^1.9.0",
		"@aws-sdk/client-s3": "^3.620.1",
		"@nurodev/astro-bun": "^1.1.3",
		"astro": "^4.12.2",
		"@sapphire/prettier-config": "^2.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/bun": "^1.1.6",
		"@typescript-eslint/parser": "^7.18.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.8.0",
		"eslint-plugin-astro": "^1.2.3",
		"husky": "^9.1.4",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.3",
		"prettier-plugin-astro": "^0.14.1",
		"pretty-quick": "^4.0.0",
		"typescript": "^5.5.4"
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wolfstar-project/cdn.wolfstar.rocks.git"
	},
	"bugs": {
		"url": "https://github.com/wolfstar-project/cdn.wolfstar.rocks/issues"
	},
	"prettier": "@sapphire/prettier-config"
}
